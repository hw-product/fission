{
    "fission": {
        "finalizer": null,
        "code_fetcher": {
            "working_directory": "/tmp/repositories"
        },
        "package_builder": {
            "chef_solo_path": "/opt/chef/bin/chef-solo",
            "working_directory": "/tmp/packages"
        },
        "utils": {
            "process": {
                "max_processes": 5,
                "enable_spawn": true
            }
        },
        "router": {
            "behavior": "config",
            "route": ["nellie"]
        },
        "rest_api": {
            "github_commit": {
                "job_name": "router"
            }
        },
        "loaders": {
            "workers": [
                "fission-router",
                "fission-validator",
                "fission-rest-api",
                "fission-code-fetcher",
                "fission-nellie"
            ],
            "sources": [
                "carnivore-http"
            ]
        },
        "rest_api": {
            "github_commit": {
                "job_name": "nellie"
            }
        },
        "sources": {
            "validator": {
                "type": "http",
                "args": {
                    "port": 3001
                }
            },
            "rest_api": {
                "type": "http",
                "args": {
                    "bind": "127.0.0.1",
                    "port": 9876
                }
            },
            "code_fetcher": {
                "type": "http",
                "args": {
                    "bind": "127.0.0.1",
                    "port": 3002
                }
            },
            "package_builder": {
                "type": "http",
                "args": {
                    "bind": "127.0.0.1",
                    "port": 3003
                }
            },
            "nellie": {
                "type": "http",
                "args": {
                    "bind": "127.0.0.1",
                    "port": 3004
                }
            },
            "router": {
                "type": "http",
                "args": {
                    "bind": "127.0.0.1",
                    "port": 3005
                }
            }
        },
        "workers": {
            "nellie": {
                "validators": 1,
                "banks": 3
            },
            "validator": {
                "github": 2
            },
            "rest_api": 0,
            "package_builder": 1,
            "code_fetcher": 1,
            "nellie": 1
        }
    }
}
